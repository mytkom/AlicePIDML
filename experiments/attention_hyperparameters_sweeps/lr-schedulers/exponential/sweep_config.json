{
  "method": "grid",
  "metric": {
    "name": "val/f1_best",
    "goal": "maximize"
  },
  "parameters": {
    "sweep": {
      "parameters": {
        "model": {
          "parameters": {
            "attention": {
              "parameters": {
                "dropout": {
                  "values": [
                    0.0,
                    0.01,
                    0.1,
                    0.2
                  ]
                }
              }
            }
          }
        },
        "training": {
          "parameters": {
            "lr_schedulers": {
              "parameters": {
                "exponential": {
                    "parameters": {
                      "gamma": {
                        "values": [0.6, 0.7, 0.8, 0.9, 0.95]
                      }
                    }
                }
              }
            },
            "start_lr": {
              "values": [
                0.001,
                0.0003,
                0.00001
              ]
            }
          }
        }
      }
    }
  }
}
